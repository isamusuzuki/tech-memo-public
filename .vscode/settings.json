{
    "files.eol": "\n",
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one",
        "editor.codeActionsOnSave": {
            "source.fixAll.markdownlint": "explicit"
        }
    },
    "markdownlint.config": {
        "MD033": {
            "allowed_elements": [
                "br",
                "img"
            ]
        }
    }
}